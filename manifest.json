{
    "author": "Flywheel <support@flywheel.io>",
    "command": "./run",
    "config": {
        "size": {
            "description": "Size of file in MB (default 1000)",
            "type": "integer",
            "default": 1000
        }
    },
    "custom": {
        "gear-builder": {
            "category": "converter",
            "image": "flywheel/debug-generatefile:v0.0.3rc1"
        },
        "docker-image": "flywheel/debug-generatefile:v0.0.3rc1"
    },
    "version": "0.0.3rc1",
    "description": "This Gear produces a .txt file with the given size in MB.",
    "environment": {
        "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
        "REQUESTS_CA_BUNDLE": "/etc/ssl/certs/ca-certificates.crt"
    },
    "inputs": {},
    "label": "Debug File Generator: Creating a large file",
    "license": "MIT",
    "maintainer": "Flywheel <support@flywheel.io>",
    "name": "debug-generatefile",
    "source": "https://github.com/flywheel-apps/debug-generatefile",
    "url": "https://github.com/flywheel-apps/debug-generatefile"
}
